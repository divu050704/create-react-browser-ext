{
  "manifest_version": 3,
  "name": "EXTENSION_NAME",
  "version": "1.0.0",
  "description": "EXTENSION_DESCRIPTION",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["<all_urls>"]
}
